-- dog info --

local frames_per_sprite=5

local function update()
 -- apply gravity to jump,
 -- unless the jump button is
 -- clicked and the dog is
 -- neither too high nor
 -- slowing down the climb.
 -- this is so the user can
 -- click longer for a higher
 -- jump.
 if dog.z<0 then
  if not (btn(‚ùé) or btn(üÖæÔ∏è))
  or dog.z<-13 -- too high
  or dog.dz>=-1.5 -- too slow
  then
   dog.dz+=0.35
  end
 end
 
 -- apply velocity deltas
 if dog.dx!=0 and dog.dy!=0 then
  -- ensure diagonal movements
  -- aren't faster than non
  -- diagonal ones
  local acc=abs(dog.dx)+abs(dog.dy)
  local acc_v=sqrt(dog.dx*dog.dx+dog.dy*dog.dy)
  local reduction=acc_v/acc
  dog.x+=dog.dx*reduction
	 dog.y+=dog.dy*reduction
 else
  dog.x+=dog.dx 
	 dog.y+=dog.dy
 end
	dog.z+=dog.dz
	
	-- dog doesn't enter the floor
	if dog.z>0 then
	 dog.z=0
	 dog.dz=0
	end
 
 -- jump if needed and on floor
 if (btnp(‚ùé) or btnp(üÖæÔ∏è))
 and dog.z==0
 then
  dog.dz=-2.4
 end
 
 -- move up and down if needed,
 -- using acceleration and
 -- deceleration for effect
 if btn(‚¨ÜÔ∏è) then
	 dog.dy-=0.35
	elseif btn(‚¨áÔ∏è) then
	 dog.dy+=0.35
	else
	 dog.dy/=1.6
	end	
	
	-- move left and right in
	-- the same way if needed
 if btn(‚û°Ô∏è) then
	 dog.dx+=0.35
	elseif btn(‚¨ÖÔ∏è) then
	 dog.dx-=0.35
	else
	 dog.dx/=1.6
	end
	
	-- to limit movement speed
	-- limit vertical
	if dog.dy<0 then
	 dog.dy=max(-1.4,dog.dy)
	elseif dog.dy>0 then
	 dog.dy=min(1.4,dog.dy)
	end
	-- limit horizontal
	if dog.dx<0 then
	 dog.dx=max(-1.4,dog.dx)
	elseif dog.dx>0 then
	 dog.dx=min(1.4,dog.dx)
	end
	
	-- update the dog sprite
	dog.sprite+=1
	if dog.sprite>6*frames_per_sprite then
	 dog.sprite=frames_per_sprite
	end
end

local function draw()
 -- draw dog shadow if in air
 --if dog.z!=0 then
	 ovalfill(
	  dog.x+1,
	  dog.y+5,
	  dog.x+6,
	  dog.y+7,
	  5 -- light gray
	 )
 --end

 -- draw dog sprite
 spr(
  dog.sprite/frames_per_sprite,
  dog.x,
  dog.y+dog.z
 )
end

dog={
 x=0,
 y=0,
 z=0,
 dx=0,
 dy=0,
 dz=0,
 sprite=frames_per_sprite,
 _update=update,
 _draw=draw,
}
