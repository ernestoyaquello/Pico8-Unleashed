function _init()
 dog.x,dog.y=50,115
 buildings={
  create_pub(-15,4),
  create_pub(-15+7*8,4),
  create_pub(-15+14*8,4),
 }
 
end

function _update60()
 dog._update()
end

function _draw()
 -- yellow for transparencies
 palt(0, false)
 palt(10, true)

 -- light gray background
 cls(6)
 
 -- draw the different elements
 map()
 for _,building in ipairs(buildings) do
   building._draw()
 end
	dog._draw()
end
